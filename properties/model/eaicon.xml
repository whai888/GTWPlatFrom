<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="eaicon">
  <select id="getTableAll" parameterType="map" resultType="hashmap">select * from eaicon</select>
  <select id="getTableAllCount" parameterType="map" resultType="int">select count(*) from eaicon</select>
  <select id="getTableByPk" parameterType="map" resultType="hashmap">select * from eaicon where 1=1 </select>
  <delete id="deleteTableByPk" parameterType="map">delete eaicon where 1=1 </delete>
</mapper>